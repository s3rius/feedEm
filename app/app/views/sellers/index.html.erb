 <f-title :title='"Sellers"'></f-title>

  <table class="table is-fullwidth">
  <thead>
    <tr>
      <th>Name</th>
      <th>Likes</th>
      <th>Dislikes</th>
      <th>Auth token</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sellers.each do |seller| %>
      <tr>
        <td><%= seller.name %></td>
        <td><%= seller.likes %></td>
        <td><%= seller.dislikes %></td>
        <td><%= seller.auth_token %></td>
        <td><%= link_to 'Show', seller %></td>
        <td><%= link_to 'Edit', edit_seller_path(seller) %></td>
        <td><%= link_to 'Destroy', seller, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Seller', new_seller_path, class: "button is-primary" %>